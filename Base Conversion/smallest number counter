#include <iostream>
#include <stdlib.h>
using namespace std;

int main() {
  int array[] = {4,2,1,2,3,4,2,1,2,2,22,1,1,2,4,6,4,4,3,4,2,2,2}
  printf
}

int min_item_count(int number_seq[], int number_seq_size) {
  number_seq_size == sizeof(number_seq[]);
  int acc = 0;
  int smlNum = number_seq[0];
  for (i = 0: i < number_seq_size, i++) {
    if (smlNum > number_seq[i])
    {
      smlNum == number_seq[i]
    }
  for (i = 0; i < number_seq_size; i++) {
    if number_seq[i] == smlNum;
    acc++
  }
  return acc;
}
